// FAHP Weight Calculation
digraph {
	node [color=lightgrey shape=rectangle style=filled]
	start [label=Start]
	input [label="Input Fuzzy Comparison Matrix"]
	geometric_mean [label="Calculate Fuzzy Geometric Means"]
	fuzzy_weights [label="Calculate Fuzzy Weights"]
	defuzzify [label="Defuzzify Fuzzy Weights"]
	normalize [label="Normalize Weights"]
	consistency_check [label="Check Consistency Ratio"]
	consistent [label="Consistent?"]
	output [label="Output FAHP Weights"]
	end [label=End]
	reenter [label="Re-enter Fuzzy Comparison Matrix"]
	start -> input
	input -> geometric_mean
	geometric_mean -> fuzzy_weights
	fuzzy_weights -> defuzzify
	defuzzify -> normalize
	normalize -> consistency_check
	consistency_check -> consistent
	consistent -> output [label=Yes]
	consistent -> reenter [label=No]
	reenter -> input
	output -> end
}
