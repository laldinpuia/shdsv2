// Soil Health Diagnostic System
digraph {
	node [color=lightgrey shape=rectangle style=filled]
	start [label=Start]
	input [label="Input Farmer Information
and Soil Health Indicators"]
	validate [label="Validate Input"]
	fahp [label="Calculate FAHP Weights"]
	assess [label="Assess Soil Health"]
	output [label="Output Soil Health Score
and Recommendations"]
	save [label="Save Results to Database"]
	generate_report [label="Generate PDF Report"]
	end [label=End]
	start -> input
	input -> validate
	validate -> fahp
	fahp -> assess
	assess -> output
	output -> save
	save -> generate_report
	generate_report -> end
}
